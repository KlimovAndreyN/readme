{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@project/account/authentication": [
        "libs/account/authentication/src/index.ts"
      ],
      "@project/account/blog-user": ["libs/account/blog-user/src/index.ts"],
      "@project/account/config": ["libs/account/config/src/index.ts"],
      "@project/account/notify": ["libs/account/notify/src/index.ts"],
      "@project/account/refresh-token": [
        "libs/account/refresh-token/src/index.ts"
      ],
      "@project/api/config": ["libs/api/config/src/index.ts"],
      "@project/blog/blog-band": ["libs/blog/blog-band/src/index.ts"],
      "@project/blog/blog-post": ["libs/blog/blog-post/src/index.ts"],
      "@project/blog/blog-post-comment": [
        "libs/blog/blog-post-comment/src/index.ts"
      ],
      "@project/blog/blog-post-like": ["libs/blog/blog-post-like/src/index.ts"],
      "@project/blog/blog-subscription": [
        "libs/blog/blog-subscription/src/index.ts"
      ],
      "@project/blog/blog-tag": ["libs/blog/blog-tag/src/index.ts"],
      "@project/blog/config": ["libs/blog/config/src/index.ts"],
      "@project/blog/models": ["libs/blog/models/src/index.ts"],
      "@project/file-storage/config": ["libs/file-storage/config/src/index.ts"],
      "@project/file-storage/file-uploader": [
        "libs/file-storage/file-uploader/src/index.ts"
      ],
      "@project/notify/config": ["libs/notify/config/src/index.ts"],
      "@project/notify/email-subsriber": [
        "libs/notify/email-subsriber/src/index.ts"
      ],
      "@project/notify/mail": ["libs/notify/mail/src/index.ts"],
      "@project/shared/core": ["libs/shared/core/src/index.ts"],
      "@project/shared/data-access": ["libs/shared/data-access/src/index.ts"],
      "@project/shared/helpers": ["libs/shared/helpers/src/index.ts"],
      "@project/shared/interceptors": ["libs/shared/interceptors/src/index.ts"],
      "@project/shared/pipes": ["libs/shared/pipes/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
